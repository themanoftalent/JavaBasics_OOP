import java.util.Scanner;


public class Main {

    public static void main(String[] args) {

        Scanner gir = new Scanner(System.in);
        int sayi;

        System.out.println("sayi gir ");
        sayi= gir.nextInt();

        if(sayi%2 == 0){
            System.out.println("cift");
        }
        else
            System.out.println("tek");
    }
}
