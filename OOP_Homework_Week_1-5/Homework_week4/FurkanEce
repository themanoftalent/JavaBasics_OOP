import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.println("Geriye dogru sayilacak sayiyi giriniz.");
        int sayi = scanner.nextInt();
        for(;sayi>0;sayi--){
            System.out.println(sayi);
        }
    }
}
