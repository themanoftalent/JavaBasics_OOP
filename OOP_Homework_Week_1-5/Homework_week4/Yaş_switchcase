package yaş;
import java.util.Scanner;
public class Yaşswitchcase {
	
	public staric void main(String[]args) {

		
	 Scanner deger = new Scanner(System.in);

     System.out.print("Bugünün yılını girin: ");
     int bugunYili = deger.nextInt();

     System.out.print("Doğum yılınızı girin: ");
     int dogumYili = deger.nextInt();

     int yas = bugunYili - dogumYili;

     switch (yas) {
         case 0,1,2,3,4,5,6:
             System.out.println(yas+"yaşındasın");
             break;
             
         case 7,8,9,10:
             System.out.println("ilkokudlasın");
             break;
             
         case 11,12,13,14:
             System.out.println("ortaokudlasın");
             break;
         case 15,16,17,18:
             System.out.println("lisedesin");
             break;
             
         case 19,20,21,22:
             System.out.println("universtedesin");
             break;
             
             default:
            	 System.out.print("yaşlısın");
            	 break;
         
             
            
             
             
 
		
	
       }
    }
}
