public class MyClassDriver {
    public static void main(String[] args) {
        
        MyClass ornek = new MyClass();
        
        System.out.println(ornek.isim);
        System.out.println(ornek.soyisim);
        System.out.println(ornek.boy);
        System.out.println(ornek.tarih);
        System.out.println(ornek.yas);
        System.out.println(ornek.hobby);
        System.out.println(ornek.uni);
        System.out.println(ornek.sacRenk);
        System.out.println(ornek.bolum);
    }
}
